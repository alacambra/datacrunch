<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>


	<table-filter match-catalog="redmine"
		match-name="activities_by_user" />
	<table-filter match-catalog="redmine" match-name="basic_info" />
	<table-filter match-catalog="redmine" match-name="enumerations" />
	<table-filter match-catalog="redmine" match-name="issues" />
	<table-filter match-catalog="redmine" match-name="projects" />
	<table-filter match-catalog="redmine" match-name="users" />
	<table-filter match-catalog="redmine"
		match-name="activities_by_issue" />
	<table-filter match-catalog="redmine"
		match-name="activities_by_project" />
	<table-filter match-catalog="redmine"
		match-name="comments_with_activity" />
	<table-filter match-catalog="redmine"
		match-name="total_time_per_user" />
	<table-filter match-name=".*"></table-filter>
</hibernate-reverse-engineering>